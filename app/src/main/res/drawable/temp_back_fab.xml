<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="oval">
                    <gradient
                        android:angle="45"
                        android:endColor="#AC332B"
                        android:startColor="#F4E040"
                        android:type="linear" />
                </shape>
            </item>

            <item>
                <shape android:shape="oval">
                    <gradient
                        android:angle="45"
                        android:endColor="#FE938C"
                        android:startColor="#FFF07C"
                        android:type="linear" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>